programa {
  funcao inicio() {
    escreva("Bimestral 1\n")
    inteiro numeroUm
    leia(numeroUm)
    escreva("Bimestral 2\n")
    inteiro numeroDois
    leia(numeroDois)
    escreva("Bimestral 3\n")
    inteiro numeroTres
    leia(numeroTres)
    escreva("Bimestral 4\n")
    inteiro numeroQuatro
    leia (numeroQuatro)
    inteiro media= (numeroUm + numeroDois + numeroTres + numeroQuatro) / 3
    escreva ("Média Bimestral:\n", media)

  }
}
